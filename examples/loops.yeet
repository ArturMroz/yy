let i = 0
yoyo i < 5 {
    let i = i + 1
}

yelp(i)


let j = 0
let result = yoyo j < 4 {
    let j = j + 1
}

yelp("result: ", result, " j: ", j)
assert(result == 4)

let arr = []
let i = 1

yoyo len(arr) < 5 {
    let arr = push(arr, i)
    let i = i + 1
}

assert(arr == [1, 2, 3, 4, 5])

yoyo len(arr) > 1 {
    yelp(arr)
    let arr = rest(arr)
}

assert(arr == [5])
